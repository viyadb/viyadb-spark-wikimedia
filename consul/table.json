{
  "name": "wikimedia",
  "deepStorePath": "/tmp/wikimedia",
  "realTime": {
    "streamSourceClass": "com.github.viyadb.spark.samples.wikimedia.WikimediaSource",
    "recordFactoryClass": "com.github.viyadb.spark.samples.wikimedia.WikimediaRecordFactory",
    "windowDuration": "PT2M"
  },
  "batch": {
    "partitioning": {
      "column": "user",
      "hashColumn": true,
      "numPartitions": 3
    }
  },
  "dimensions": [
    {"name": "channel"},
    {"name": "time", "type": "time", "granularity": "day", "format": "%Y-%m-%d"},
    {"name": "title"},
    {"name": "flags"},
    {"name": "diffUrl"},
    {"name": "user"},
    {"name": "byteDiff"},
    {"name": "summary"}
  ],
  "metrics": [
    {"name": "count", "type": "count"}
  ],
  "timeColumn": {"name": "time"}
}
